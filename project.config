[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group regot
	owner = group user/rebekkaa
[access "refs/for/refs/*"]
	read = group regot
	read = group user/rebekkaa
	push = group regot
	push = group user/rebekkaa
	label-Code-Review = -2..+2 group regot
	label-Code-Review = -2..+2 group user/rebekkaa
	label-Verified = -1..+1 group regot
	label-Verified = -1..+1 group user/rebekkaa
	rebase = group regot
	rebase = group user/rebekkaa
	removeReviewer = group regot
	removeReviewer = group user/rebekkaa
	submit = group regot
	submit = group user/rebekkaa
[access "refs/heads/*"]
	read = group regot
	read = group user/rebekkaa
	create = group regot
	create = group user/rebekkaa
	push = group regot
	push = +force group user/rebekkaa
	pushMerge = group regot
	pushMerge = group user/rebekkaa
[access "refs/tags/*"]
	push = group regot
	push = group user/rebekkaa
	push = +force group user/rebekkaa
